<div class="container">
  <%= form_with url:send_form_path, method: :post do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name%>
    <%= f.label :last_name %>
    <%= f.text_field :last_name%>
    <%= f.label :email %>
    <%= f.text_field :email%>
    <%= f.label :description %>
    <%= f.text_field :description%>
    <%= f.label :about %>
    <%= f.text_field :about %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
    <%= f.submit %>
  <% end %>
</div>
